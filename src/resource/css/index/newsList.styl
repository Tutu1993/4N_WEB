.list_head
	position relative
	width 100%
	height 30px
	span
		font-size 15px
		font-weight 400
		font-family tbold,arial,sans-serif
.list_logo
	position absolute
	width 52px
	height 20px
	top 0
	right 0
	a
		display inline-block
		width 20px
		height 20px
		&:first-child
			background url('../../imgs/index/tw-g.svg') no-repeat center center
			margin-right 12px
		&:last-child
			background url('../../imgs/index/fb-g.svg') no-repeat center center
.list_title
	font-size 34px
	line-height 52px
	font-weight 400
	font-family tlight,'arial narrow',sans-serif
	margin-bottom 2px
	text-transform uppercase
.list_img
	width 100%
	height 234px
	background url('../../imgs/index/161118104010.jpg') no-repeat center center
	background-size contain
.list_link
	position absolute
	display block
	width 90px
	height 12px
	bottom 0
	left 0
	font-size 10px
	font-weight 400
	font-family: treg,arial,sans-serif;
	letter-spacing 1px
	padding 7px 7px 6px 8px
	border-radius 3px
	background #f6f6f6
	color #252525
	user-select none
	text-align center
	text-transform uppercase
	text-decoration none
